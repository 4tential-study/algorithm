#include<stdio.h>

int arr[200001];
int hap[200001];
int numofwork;

void init(){
	int i, sum=0;
	for(i=1; i<=numofwork; ++i){
		sum+=arr[i];
		hap[i] = sum;
	}
}

int yeeboon(int start, int end, int find){
	if(start==end){
		if(hap[start]==find) return start;
		else if(hap[start]>find) return start-1;
		else{
			if(start==numofwork) return start;
			else start+1;
		}
	}
	int mid = (start+end)/2;
	//if(hap[mid]==find) return mid;
	if(hap[mid]>=find) return yeeboon(start, mid, find);
	else return yeeboon(mid+1, end, find);
}

int solve(int time){
	int dap = yeeboon(1,numofwork, time);
	return dap;
}

int main(void)
{
	int query;
	scanf("%d %d", &numofwork, &query);
	int i;
	for(i=1; i<=numofwork; ++i){
		scanf("%d", &arr[i]);
	}
	init();
	int time;
	for(i=0; i<query; ++i){
		scanf("%d", &time);
		printf("%d\n", solve(time));
	}
}
